generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model album {
  id     BigInt  @id(map: "idx_46381_primary") @default(autoincrement())
  title  String  @db.VarChar(191)
  artist String  @db.VarChar(191)
  cover  String  @db.VarChar(191)
  track  track[]
}

model track {
  id       BigInt @id(map: "idx_46388_primary") @default(autoincrement())
  title    String @db.VarChar(191)
  duration BigInt
  albumid  BigInt
  album    album  @relation(fields: [albumid], references: [id])

  @@index([albumid], map: "idx_46388_track_albumid_fkey")
}
