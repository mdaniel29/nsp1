generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider  = "postgresql"
}

model album {
  id     Int  @id(map: "idx_18608_primary") @default(autoincrement())
  title  String  @db.VarChar(191)
  artist String  @db.VarChar(191)
  cover  String  @db.VarChar(191)
  track  track[]
}

model track {
  id       Int @id(map: "idx_18615_primary") @default(autoincrement())
  title    String @db.VarChar(191)
  duration Int
  albumid  Int
  album    album  @relation(fields: [albumid], references: [id])

  @@index([albumid], map: "idx_18615_track_albumid_fkey")
}